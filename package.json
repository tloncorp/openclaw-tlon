{
  "name": "@openclaw/tlon",
  "version": "2026.1.30",
  "description": "OpenClaw Tlon/Urbit channel plugin",
  "type": "module",
  "scripts": {
    "format": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "lint": "oxlint --type-aware",
    "lint:fix": "pnpm format:fix && oxlint --type-aware --fix",
    "test": "vitest run",
    "test:security": "vitest run src/security.test.ts",
    "test:watch": "vitest",
    "build:cli": "esbuild src/cli/index.ts --bundle --platform=node --format=esm --outfile=skills/tlon/scripts/tlon.mjs --external:@tloncorp/api",
    "build:cli:standalone": "esbuild src/cli/index.ts --bundle --platform=node --format=esm --outfile=skills/tlon/scripts/tlon.mjs"
  },
  "dependencies": {
    "@urbit/aura": "^3.0.0",
    "@urbit/http-api": "^3.0.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.2",
    "openclaw": "^2026.1.30",
    "oxfmt": "0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.11.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0",
    "zod": "^4.3.6"
  },
  "openclaw": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "tlon",
      "label": "Tlon",
      "selectionLabel": "Tlon (Urbit)",
      "docsPath": "/channels/tlon",
      "docsLabel": "tlon",
      "blurb": "decentralized messaging on Urbit; install the plugin to enable.",
      "order": 90,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@moltbot/tlon",
      "localPath": "extensions/tlon",
      "defaultChoice": "npm"
    }
  }
}
