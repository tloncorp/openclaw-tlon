{
  "name": "@openclaw/tlon",
  "version": "2026.1.30",
  "description": "OpenClaw Tlon/Urbit channel plugin",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "openclaw.plugin.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm clean && pnpm build",
    "dev": "./dev/run.sh",
    "format": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "lint": "oxlint --type-aware",
    "lint:fix": "pnpm format:fix && oxlint --type-aware --fix",
    "test": "vitest run",
    "test:security": "vitest run src/security.test.ts",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@tloncorp/api": "github:tloncorp/api-beta#main",
    "@tloncorp/tlon-skill": "0.1.9",
    "@urbit/aura": "^3.0.0",
    "@urbit/http-api": "^3.0.0"
  },
  "devDependencies": {
    "openclaw": "^2026.1.30",
    "oxfmt": "0.27.0",
    "oxlint": "^1.42.0",
    "oxlint-tsgolint": "^0.12.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.0",
    "zod": "^4.3.6"
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ],
    "channel": {
      "id": "tlon",
      "label": "Tlon",
      "selectionLabel": "Tlon (Urbit)",
      "docsPath": "/channels/tlon",
      "docsLabel": "tlon",
      "blurb": "decentralized messaging on Urbit; install the plugin to enable.",
      "order": 90,
      "quickstartAllowFrom": true
    },
    "install": {
      "npmSpec": "@openclaw/tlon",
      "localPath": "extensions/tlon",
      "defaultChoice": "npm"
    }
  }
}
