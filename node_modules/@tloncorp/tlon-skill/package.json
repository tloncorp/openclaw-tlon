{
  "name": "@tloncorp/tlon-skill",
  "version": "0.1.9",
  "description": "Tlon/Urbit skill for OpenClaw agents",
  "type": "module",
  "bin": {
    "tlon": "./bin/tlon.js"
  },
  "files": [
    "bin/",
    "scripts/postinstall.js",
    "SKILL.md"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:all": "node scripts/build-all.js",
    "dev:link": "bun run build:all && cp \"npm/$(node -e 'console.log(process.platform + \"-\" + process.arch)')/tlon\" bin/",
    "test": "bun test",
    "version": "node scripts/bump-version.js",
    "postinstall": "node scripts/postinstall.js"
  },
  "optionalDependencies": {
    "@tloncorp/tlon-skill-darwin-arm64": "0.1.9",
    "@tloncorp/tlon-skill-darwin-x64": "0.1.9",
    "@tloncorp/tlon-skill-linux-x64": "0.1.9",
    "@tloncorp/tlon-skill-linux-arm64": "0.1.9"
  },
  "devDependencies": {
    "@tloncorp/api": "git+https://github.com/tloncorp/api-beta.git#main",
    "@urbit/aura": "^3.0.0",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tloncorp/tlon-skill.git"
  },
  "license": "MIT"
}
